{
  "scripts": {
    "client": "cd client && npm install && npm start",
    "server": "cd server && npm install && nodemon --watch 'src/**/*.ts' --exec 'ts-node' index.ts",
    "start": "concurrently \"npm run server\" \"npm run client\""
  },
  "devDependencies": {
    "@types/express-rate-limit": "^6.0.0",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^1.6.0",
    "express-rate-limit": "^7.1.3",
    "install": "^0.13.0",
    "nodeman": "^1.1.2",
    "nodemon": "^3.0.1",
    "npm": "^10.2.3"
  }
}
